<!DOCTYPE html>
<html>
<head>
    @RenderSection("scripts", required: false)
    <title>@ViewBag.Title</title>
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL STYLES -->
    <link href="~/Scripts/theme/assets/admin/pages/css/login.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME STYLES -->
    <link href="~/Scripts/theme/assets/global/css/components.css" id="style_components" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/global/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/admin/layout/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/theme/assets/admin/layout/css/themes/darkblue.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="~/Scripts/theme/assets/admin/layout/css/custom.css" rel="stylesheet" type="text/css" />
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<body>
    <div class="container body-content">
        @RenderBody()
    </div>
    <script src="~/Scripts/theme/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/global/plugins/jquery.cokie.min.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/global/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="~/Scripts/theme/assets/global/plugins/jquery-validation/js/jquery.validate.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/Scripts/theme/assets/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/admin/layout/scripts/layout.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/admin/layout/scripts/demo.js" type="text/javascript"></script>
    <script src="~/Scripts/theme/assets/admin/pages/scripts/login.js" type="text/javascript"></script>
    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            Login.init();
            Demo.init();
        });
    </script>
</body>
</html>

